<div id="schedule-page">
  <div class="page-header page-header-small" filter-color="orange">
    <div class="page-header-image" data-parallax="true" style="background-image: url('<%= static_path(@conn, "/images/calendar.jpg") %>'); transform: translate3d(0px, 0px, 0px);">
    </div>
    <div class="container">
      <div class="content-center">
        <h3 class="title">Training Schedule</h3>
        <h3><small>As of June 2017</small></h3>
        <div class="content">
          <p class="description">
            Our schedule below is flexible and fits to your availability.
          </p>
          <p class="description">
            You may train as much as you want for no additional charges.
          </p>
        </div>
      </div>
    </div>
  </div>
  <div class="gold-gradient"></div>
  <div class="section">
    <div class="container">
      <div class="row">
        <div class="schedule-pdf-wrapper">
          <iframe src="https://s3-us-west-2.amazonaws.com/kenshusei-martial-arts/Zen_Schedule.pdf" style="margin:auto;" frameborder="0"></iframe>
        </div> 
      </div>
      <div class="schedule-image-left rellax" data-rellax-speed="-3" data-rellax-percentage="1.5">
        <img src="<%= static_path(@conn, "/images/Zen2.png") %>">
      </div>
      <div class="schedule-image-right rellax" data-rellax-speed="3" data-rellax-percentage="1.5">
        <img src="<%= static_path(@conn, "/images/Zen1.png") %>">
      </div>
    </div>
  </div>
</div>